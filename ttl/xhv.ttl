@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
@prefix rdfa: <http://www.w3.org/ns/rdfa#>
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
@prefix role: <http://www.w3.org/1999/xhtml/vocab#role>
@prefix xhv: <http://www.w3.org/1999/xhtml/vocab#>

<http://www.w3.org/1999/xhtml/vocab> xhv:stylesheet <http://www.w3.org/StyleSheets/TR/base.css>.

xhv:alert
  a rdf:Property;
  rdfs:comment """A message
      with important, and usually time-sensitive, information. Also see alertdialog
      and status.""".

xhv:alertdialog
  a rdf:Property;
  rdfs:comment """A
      type of dialog that contains an alert message, where initial focus goes
      an element within the dialog. Also see alert and dialog.""".

xhv:alternate
  a rdf:Property;
  rdfs:comment """alternate
    designates alternate versions for a resource.""";
  rdfs:member xhv:relrev-properties;
  rdfa:term "alternate";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#alternate".

xhv:appendix
  a rdf:Property;
  rdfs:comment """appendix refers to a resource serving
      as an appendix in a collection. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "appendix";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#appendix".

xhv:application
  a rdf:Property;
  rdfs:comment """A
      region declared as a web application, as opposed to a web document.""".

xhv:article
  a rdf:Property;
  rdfs:comment """A section
      of a page that consists of a composition that forms an independent part
      of a document, page, or site.""".

xhv:banner
  a rdf:Property;
  rdfs:comment """contains
      the prime heading or internal title of a page. """;
  rdfs:member xhv:relrev-properties.

xhv:bookmark
  a rdf:Property;
  rdfs:comment """bookmark refers to a bookmark - a link
      to a key entry point within an extended document. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "bookmark";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#bookmark".

xhv:button
  a rdf:Property;
  rdfs:comment """An input
      that allows for user-triggered actions when clicked or pressed. Also see
      link.""".

xhv:chapter
  a rdf:Property;
  rdfs:comment """chapter refers to a resource serving
      as a chapter in a collection. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "chapter";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#chapter".

xhv:checkbox
  a rdf:Property;
  rdfs:comment """A checkable
      input that has three possible values: true, false, or mixed.""".

xhv:cite
  a rdf:Property;
  rdfs:comment """cite refers to a resource that defines
      a citation. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "cite";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#cite".

xhv:columnheader
  a rdf:Property;
  rdfs:comment """A
      cell containing header information for a column.""".

xhv:combobox
  a rdf:Property;
  rdfs:comment """A presentation
      of a select; usually similar to a textbox where users can type ahead to
      select an option, or type to enter arbitrary text as a new item in the
      list. Also see listbox.""".

xhv:complementary
  a rdf:Property;
  rdfs:comment """indicates that the section supports but is
      separable from the main content of resource.""";
  rdfs:member xhv:relrev-properties.

xhv:contentinfo
  a rdf:Property;
  rdfs:comment """contains meta information about the
      content on the page or the page as a whole.""";
  rdfs:member xhv:relrev-properties.

xhv:contents
  a rdf:Property;
  rdfs:comment """contents refers to a resource serving
      as a table of contents. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "contents";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#contents".

xhv:copyright
  a rdf:Property;
  rdfs:comment """copyright refers to a copyright
      statement for the resource. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "copyright";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#copyright".

xhv:definition
  a rdf:Property;
  rdfs:comment "indicates the definition of a term or concept.";
  rdfs:member xhv:relrev-properties.

xhv:dialog
  a rdf:Property;
  rdfs:comment """A dialog
      is an application window that is designed to interrupt the current processing
      of an application in order to prompt the user to enter information or require
      a response. Also see alertdialog.""".

xhv:directory
  a rdf:Property;
  rdfs:comment """A list
      of references to members of a group, such as a static table of contents.""".

xhv:document
  a rdf:Property;
  rdfs:comment """A region
      containing related information that is declared as document content, as
      opposed to a web application.""".

xhv:first
  a rdf:Property;
  rdfs:comment """first refers the first item in a
      collection (see also start and top).""";
  rdfs:member xhv:relrev-properties;
  rdfa:term "first";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#first".

xhv:form
  a rdf:Property;
  rdfs:comment """A landmark
      region that contains a collection of items and objects that, as a whole,
      combine to create a form. Also see search.""".

xhv:glossary
  a rdf:Property;
  rdfs:comment """glossary refers to a resource
      providing a glossary of terms. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "glossary";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#glossary".

xhv:grid
  a rdf:Property;
  rdfs:comment """A grid is
      an interactive control which contains cells of tabular data arranged in
      rows and columns, like a table.""".

xhv:gridcell
  a rdf:Property;
  rdfs:comment """A cell
      in a grid or treegrid.""".

xhv:group
  a rdf:Property;
  rdfs:comment """A set of
      user interface objects which are not intended to be included in a page
      summary or table of contents by assistive technologies.""".

xhv:heading
  a rdf:Property;
  rdfs:comment """A heading
      for a section of the page.""".

xhv:help
  a rdf:Property;
  rdfs:comment """help refers to a resource offering
      help. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "help";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#help".

xhv:icon
  a rdf:Property;
  rdfs:comment """icon refers to a resource that
      represents an icon. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "icon";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#icon".

xhv:img
  a rdf:Property;
  rdfs:comment """A container
      for a collection of elements that form an image.""".

xhv:index
  a rdf:Property;
  rdfs:comment """index refers to a resource providing
      an index. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "index";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#index".

xhv:itsRules
  a rdf:Property;
  rdfs:comment """itsRules indicates that the designated
      resource is an [ITS] rule set.""";
  rdfs:member xhv:relrev-properties;
  rdfa:term "itsRules";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#itsRules".

xhv:last
  a rdf:Property;
  rdfs:comment """last refers to the last resource in a
      collection of resources. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "last";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#last".

xhv:license
  a rdf:Property;
  rdfs:comment """license refers to a resource that
      defines the associated license. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "license";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#license".

xhv:link
  a rdf:Property;
  rdfs:comment """An interactive
      reference to an internal or external resource that, when activated, causes
      the user agent to navigate to that resource. Also see button.""".

xhv:list
  a rdf:Property;
  rdfs:comment """A group of
      non-interactive list items. Also see listbox.""".

xhv:listbox
  a rdf:Property;
  rdfs:comment """A widget
      that allows the user to select one or more items from a list of choices.
      Also see combobox and list.""".

xhv:listitem
  a rdf:Property;
  rdfs:comment """A single
      item in a list or directory.""".

xhv:log
  a rdf:Property;
  rdfs:comment """A type of
      live region where new information is added in meaningful order and old
      information may disappear. Also see marquee.""".

xhv:main
  a rdf:Property;
  rdfs:comment """acts as the
      main content of the document. """;
  rdfs:member xhv:relrev-properties.

xhv:marquee
  a rdf:Property;
  rdfs:comment """A type
      of live region where non-essential information changes frequently. Also
      see log.""".

xhv:math
  a rdf:Property;
  rdfs:comment """Content that
      represents a mathematical expression. """.

xhv:menu
  a rdf:Property;
  rdfs:comment """A type of
      widget that offers a list of choices to the user.""".

xhv:menubar
  a rdf:Property;
  rdfs:comment """A presentation
      of menu that usually remains visible and is usually presented horizontally.""".

xhv:menuitem
  a rdf:Property;
  rdfs:comment """An option
      in a group of choices contained by a menu or menubar.""".

xhv:menuitemcheckbox
  a rdf:Property;
  rdfs:comment """A
      checkable menuitem that has three possible values: true, false, or mixed.""".

xhv:menuitemradio
  a rdf:Property;
  rdfs:comment """A
      checkable menuitem in a group of menuitemradio roles, only one of which
      can be checked at a time.""".

xhv:meta
  a rdf:Property;
  rdfs:comment """meta refers to a resource that
      provides metadata. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "meta";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#meta".

xhv:navigation
  a rdf:Property;
  rdfs:comment """indicates a collection of items suitable for
      navigating the document or related documents.""";
  rdfs:member xhv:relrev-properties.

xhv:next
  a rdf:Property;
  rdfs:comment """next refers to the next resource
      (after the current one) in an ordered collection of resources. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "next";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#next".

xhv:note
  a rdf:Property;
  rdfs:comment """indicates
      the content is parenthetic or ancillary to the main content of the
      resource. """;
  rdfs:member xhv:relrev-properties.

xhv:option
  a rdf:Property;
  rdfs:comment """A selectable
      item in a select list.""".

xhv:p3pv1
  a rdf:Property;
  rdfs:comment """p3pv1 refers to a P3P Policy Reference
      File [P3P]. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "p3pv1";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#p3pv1".

xhv:presentation
  a rdf:Property;
  rdfs:comment """An
      element whose implicit native role semantics will not be mapped to the
      accessibility API.""".

xhv:prev
  a rdf:Property;
  rdfs:comment """prev refers to a previous resource
      (before the current one) in an ordered collection of resources. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "prev";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#prev".

xhv:progressbar
  a rdf:Property;
  rdfs:comment """An
      element that displays the progress status for tasks that take a long time.""".

xhv:radio
  a rdf:Property;
  rdfs:comment """A checkable
      input in a group of radio roles, only one of which can be checked at a
      time.""".

xhv:radiogroup
  a rdf:Property;
  rdfs:comment """A group
      of radio buttons.""".

xhv:region
  a rdf:Property;
  rdfs:comment """A large
      perceivable section of a web page or document, that the author feels is
      important enough to be included in a page summary or table of contents,
      for example, an area of the page containing live sporting event statistics.""".

xhv:relrev-properties
  a rdf:Bag;
  rdfs:member xhv:relrev-properties.

role:
  a rdf:Bag, rdf:Property;
  rdfs:comment """role indicates the purpose of the
      resource. See the XHTML Role
      Vocabulary for roles in this vocabulary space, and XHTMLROLE for information on extending the
      collection of roles. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "role";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#role".

xhv:relrev-properties
  a rdf:Bag, rdfs:member.

xhv:row
  a rdf:Property;
  rdfs:comment """A row of cells
      in a grid.""".

xhv:rowgroup
  a rdf:Property;
  rdfs:comment """A group
      containing one or more row elements in a grid.""".

xhv:rowheader
  a rdf:Property;
  rdfs:comment """A cell
      containing header information for a row in a grid.""".

xhv:scrollbar
  a rdf:Property;
  rdfs:comment """A graphical
      object that controls the scrolling of content within a viewing area, regardless
      of whether the content is fully displayed within the viewing area.""".

xhv:search
  a rdf:Property;
  rdfs:comment """indicates
      that the section provides a search facility. """;
  rdfs:member xhv:relrev-properties.

xhv:section
  a rdf:Property;
  rdfs:comment """section refers to a resource serving
      as a section in a collection. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "section";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#section".

xhv:separator
  a rdf:Property;
  rdfs:comment """A divider
      that separates and distinguishes sections of content or groups of menuitems.""".

xhv:slider
  a rdf:Property;
  rdfs:comment """A user
      input where the user selects a value from within a given range.""".

xhv:spinbutton
  a rdf:Property;
  rdfs:comment """A form
      of range that expects a user to select from amongst discrete choices.""".

xhv:start
  a rdf:Property;
  rdfs:comment """start refers to the first resource in
      a collection of resources. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "start";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#start".

xhv:status
  a rdf:Property;
  rdfs:comment """A container
      whose content is advisory information for the user but is not important
      enough to justify an alert. Also see alert.""".

xhv:stylesheet
  a rdf:Property;
  rdfs:comment """stylesheet refers to a resource
      serving as a stylesheet for a resource. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "stylesheet";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#stylesheet".

xhv:subsection
  a rdf:Property;
  rdfs:comment """subsection refers to a resource
      serving as a subsection in a collection. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "subsection";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#subsection".

xhv:tab
  a rdf:Property;
  rdfs:comment """A grouping
      label providing a mechanism for selecting the tab content that is to be
      rendered to the user.""".

xhv:tablist
  a rdf:Property;
  rdfs:comment """A list
      of tab elements, which are references to tabpanel elements.""".

xhv:tabpanel
  a rdf:Property;
  rdfs:comment """A container
      for the resources associated with a tab, where each tab is contained in
      a tablist.""".

xhv:textbox
  a rdf:Property;
  rdfs:comment """Input
      that allows free-form text as its value.""".

xhv:timer
  a rdf:Property;
  rdfs:comment """A type of
      live region containing a numerical counter which indicates an amount of
      elapsed time from a start point, or the time remaining until an end point.""".

xhv:toolbar
  a rdf:Property;
  rdfs:comment """A collection
      of commonly used function buttons represented in compact visual form.""".

xhv:tooltip
  a rdf:Property;
  rdfs:comment """A contextual
      popup that displays a description for an element.""".

xhv:top
  a rdf:Property;
  rdfs:comment "top is a synonym for start. ";
  rdfs:member xhv:relrev-properties;
  rdfa:term "top";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#top".

xhv:tree
  a rdf:Property;
  rdfs:comment """A type of
      list that may contain sub-level nested groups that can be collapsed and
      expanded.""".

xhv:treegrid
  a rdf:Property;
  rdfs:comment """A grid
      whose rows can be expanded and collapsed in the same manner as for a tree.""".

xhv:treeitem
  a rdf:Property;
  rdfs:comment """An option
      item of a tree. This is an element within a tree that may be expanded or
      collapsed if it contains a sub-level group of treeitems.""".

xhv:up
  a rdf:Property;
  rdfs:comment """up refers to a resource "above" in a
      hierarchically structured set. """;
  rdfs:member xhv:relrev-properties;
  rdfa:term "up";
  rdfa:uri "http://www.w3.org/1999/xhtml/vocab#up".
