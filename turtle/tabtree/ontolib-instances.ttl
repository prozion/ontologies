:void
  a o:Ontology ;
  dc:description "A set of RDF triples that are published, maintained or aggregated by a single provider." ;
  o:uri <http://rdfs.org/ns/void#> ;
  o:ttl <http://vocab.deri.ie/void.ttl> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/void.ttl> ;
  o:uses :rdf, :owl, :adms, :dct, :xsd, :vann, :foaf, :dc, :rdfs, :sdo .

:vs
  a o:Ontology ;
  dc:title "SemWeb Vocab Status ontology" ;
  o:uri <http://www.w3.org/2003/06/sw-vocab-status/ns#> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/vs.ttl> ;
  owl:sameAs :sw_vocab_status, :st ;
  o:uses :dc, :owl, :rdf, :xml, :xsd, :foaf, :rdfs, :vann .

:wdrs
  a o:Ontology ;
  dc:title "Protocol for Web Description Resources (POWDER): POWDER-S Vocabulary (WDRS)" ;
  o:uri <http://www.w3.org/2007/05/powder-s#> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/wdrs.ttl> ;
  o:uses :owl, :rdfs, :dc, :vann, :rdf, :xsd .

:wot
  a o:Ontology ;
  dc:title "Web Of Trust vocabulary" ;
  o:uri <http://xmlns.com/wot/0.1/> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/wot.ttl> ;
  o:uses :dc, :vs, :owl, :rdf, :xml, :xsd, :rdfs .

:xhv
  a o:Ontology ;
  dc:description "A vocabulary collection utilized by XHTML Family modules and document types" ;
  o:uri <http://www.w3.org/1999/xhtml/vocab#> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/xhv.ttl> ;
  o:uses :rdf, :rdfa, :rdfs .

:xml
  a o:Schema ;
  o:uri <http://www.w3.org/XML/1998/namespace> .

:xsd
  a o:Schema ;
  o:uri <http://www.w3.org/2001/XMLSchema#> .
