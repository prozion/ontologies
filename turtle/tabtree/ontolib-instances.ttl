@prefix : <https://github.com/prozion/oforo/data/ontologies#> .
@prefix o: <https://github.com/prozion/oforo/scheme#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

@base <https://github.com/prozion/oforo/data/ontologies#> .

:adms
  a o:Ontology ;
  dc:title "Asset Description Metadata Schema (ADMS)" ;
  o:uri <http://www.w3.org/ns/adms#> ;
  o:ttl <http://www.w3.org/ns/adms#> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/adms.ttl> ;
  o:uses :dcterms, :skos, :foaf, :owl, :xsd, :xhv, :xml, :voaf, :vcard, :rdfs, :vann, :dcat, :rdf, :wdrs, :schema .

:arg
  a o:Ontology ;
  o:comment "Where is its source? There is 404 on http://spinrdf.org/arg#. But in some other ttls you can see such properties from this ontology like arg:limit or arg:inputStream." ;
  o:uri <http://spinrdf.org/arg#> .

:bibo
  a o:Ontology ;
  dc:title "The Bibliographic Ontology" ;
  o:uri <http://purl.org/ontology/bibo/> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/bibo.ttl> ;
  o:uses :owl, :dc, :skos, :rdfs, :xsd, :vs, :rdf, :foaf .

:bio
  a o:Ontology ;
  dc:title "A vocabulary for biographical information" ;
  o:uri <http://purl.org/vocab/bio/0.1/> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/bio.ttl> ;
  o:uses :cc, :dc, :ov, :st, :bio, :dct, :owl, :rdf, :xml, :xsd, :foaf, :rdfs, :skos, :vann, :label .

:bko
  a o:Ontology ;
  o:uri <http://sbgnbricks.org/BKO/> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/bko.ttl> ;
  o:uses :owl, :xsd, :rdfs .

:cc
  a o:Ontology ;
  o:uri <http://web.resource.org/cc/> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/cc.ttl> ;
  o:uses :dc, :owl, :rdf, :xml, :xsd, :rdfs .

:daml
  a o:Ontology ;
  dc:description "DAML+OIL is a semantic markup language for Web resources" ;
  o:uri <http://www.w3.org/2001/10/daml+oil#> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/daml.ttl> ;
  o:uses :rdf, :rdfs .

:dc
  a o:Ontology ;
  dc:title "Dublin Core Metadata Element Set" ;
  o:uri <http://purl.org/dc/elements/1.1/> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/dcelements.ttl> ;
  owl:sameAs :dcelements, :dc11 ;
  o:uses :rdf, :owl, :skos, :dcam, :dcterms, :rdfs .

:dcam
  a o:Ontology ;
  dc:title "Metadata terms related to the DCMI Abstract Model" ;
  o:uri <http://www.w3.org/1999/02/22-rdf-syntax-ns#> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/dcam.ttl> ;
  o:uses :rdf, :owl, :skos, :dcam, :dcterms, :rdfs .

:dcat
  a o:Ontology ;
  dc:title "Data Catalog Vocabulary" ;
  o:uri <http://www.w3.org/ns/dcat#> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/dcat.ttl> ;
  o:uses :dct, :dctype, :foaf, :owl, :prov, :rdf, :rdfs, :sdo, :skos, :vcard, :xsd .

:dcterms
  a o:Ontology ;
  o:ttl <https://www.dublincore.org/specifications/dublin-core/dcmi-terms/dublin_core_terms.ttl> ;
  o:uri <http://purl.org/dc/terms/> ;
  o:other-ttl <https://github.com/prozion/ontologies/blob/master/ttl/dcterms.ttl> ;
  owl:sameAs :dct ;
  o:uses :rdf, :owl, :skos, :dcam, :rdfs .

:dctype
  a o:Ontology ;
  dc:title "DCMI Type Vocabulary" ;
  o:uri <http://purl.org/dc/dcmitype/> ;
